    include ../../components/header/header
    +header({ linksHeader: linksHeader, contactUs: '#contactUs' })
    main
      include ../../components/upperGradientMain/upperGradientMain
      include ../../components/solutions/solutions
      include ../../components/contactUs/contactUs
      +contactUs({title: 'Optimize Your Procurement with ApolloRise', subtitle: 'Contact us to explore AI solutions for smarter sourcing.'})
    include ../../components/footer/footer
    +footer({ linksFooter: linksFooter, contactUs: '#contactUs' })
