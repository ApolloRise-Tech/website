html
  head
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="mobile-web-app-capable" content="yes")

    <!-- Status bar styles -->
    meta(name="apple-mobile-web-app-status-bar-style" content="black-translucent")
    meta(name="theme-color" content="#FCFBF8")

    <!-- Title -->
    title AI-Powered Procurement Solutions | ApolloRise
    meta(name="description" content="Explore AI-driven solutions for contract compliance, procurement intelligence, and Merger & Acquisition due diligence. Streamline sourcing with ApolloRise.")
    meta(name="keywords" content="AI-powered procurement, contract performance, procurement intelligence, strategic consulting, execution development, Merger & Acquisition due diligence")
    meta(name="author" content="ApolloRise Tech")

    <!-- Open Graph -->
    meta(property="og:type" content="website")
    meta(property="og:title" content="AI-Powered Procurement Solutions | ApolloRise")
    meta(property="og:description" content="Explore AI-driven solutions for contract compliance, procurement intelligence, and M&A due diligence. Streamline sourcing with ApolloRise.")
    meta(property="og:url" content="https://apollorise.tech/")
    meta(property="og:image" content="https://apollorise.tech/sources/images/picture_1.png")
    meta(property="og:site_name" content="apollorise.tech")

    link(rel='stylesheet', href='../../sources/fonts/fonts.css')

    <!-- Favicons -->
    link(rel="icon" type="image/png" href=require('../../sources/images/favicon.png'))
    link(rel="shortcut icon" href=require('../../sources/images/favicon.ico'))

    link(rel="apple-touch-icon" sizes="76x76" href=require('../../sources/images/apple-touch-icon-76x76.png'))
    link(rel="apple-touch-icon" sizes="120x120" href=require('../../sources/images/apple-touch-icon-120x120.png'))
    link(rel="apple-touch-icon" sizes="152x152" href=require('../../sources/images/apple-touch-icon-152x152.png'))
    link(rel="apple-touch-icon" sizes="180x180" href=require('../../sources/images/apple-touch-icon-180x180.png'))

